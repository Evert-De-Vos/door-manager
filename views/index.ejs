<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='../public/stylesheets/style.css' />
  </head>
  <body>
    <div>
      <form class="form-home" action="">
        <input class="textbox-home-page" type="text" name="roomID" placeholder="Room id" > <br>
        <input class="submit-home-page" type="submit" value="Find Room">
      </form>
    </div>

    <script>
     function initMap() {
       var uluru = {lat: -25.363, lng: 131.044};
       var map = new google.maps.Map(document.getElementById('map'), {
         zoom: 4,
         center: uluru
       });
       var marker = new google.maps.Marker({
         position: uluru,
         map: map
       });
     }
   </script>
   <script async defer
   src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjqE0sD05focNbrGROjiUykgQr4Hbh3C0&callback=initMap">
   </script>
  </body>
</html>
